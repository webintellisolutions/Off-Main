  "scripts": {
    "build": "tsc --sourceMap false",
    "devbuild": "tsc --sourceMap true",
    "watch": "tsc -w",
    "start": "nodemon --exec \"node --max-old-space-size=8192 dist/server.js\"",
    "dev": "tsc-watch --onSuccess \"node --trace-warnings ./dist/server.js\"",
    "dev:server": "ts-node-dev --respawn --transpile-only --ignore node_modules src/server.ts",
    "db:migrate": "npx sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "pretest": "NODE_ENV=test sequelize db:migrate && NODE_ENV=test sequelize db:seed:all",
    "test": "NODE_ENV=test jest",
    "posttest": "NODE_ENV=test sequelize db:migrate:undo:all",
    "lint": "eslint src/**/*.ts"
