{
  "presets": [
      "@babel/preset-typescript",
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]   
  ],
  "plugins": [
      ["@babel/plugin-transform-react-jsx", { "throwIfNamespace": false }],
      "@babel/plugin-transform-private-property-in-object"
    ]
}